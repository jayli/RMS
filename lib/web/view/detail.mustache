      <div class="app-list">
        <h3>
        <ul class="breadcrumb">
          <li><a href="/{{apptype-href}}/{{app-id}}">{{app-name}}</a> <span class="divider">/</span>
          {{#pathes}}
          <li><a href="/{{apptype-href}}/{{app-id}}/show/{{.}}/">{{.}}</a> <span class="divider">/</span></li>
          {{/pathes}}
          {{#app-filename}}<li class="active">{{app-filename}}</li>{{/app-filename}}
        </ul>
        </h3>
        {{#svn}}
        <table class="zebra-striped">
          <thead>
            <tr>
                <th class="choose">选中</th>
                <th class="filename">文件名</th>
                <th class="version">最新版本号</th>
                <th class="version">发布版本号</th>
            </tr>
          </thead>
          <tbody>
            {{#dir}}
            <tr class="dir">
                <td>#</td>
                <td><a href="/{{apptype-href}}/{{app-id}}/show/{{prefix}}{{href}}">{{href}}</a></td>
                <td>{{newest-version}}</td>
                <td>{{last-version}}</td>
            </tr>
            {{/dir}}
            {{#file}}
            <tr>
                <td>#</td>
                <td><a href="/{{apptype-href}}/{{app-id}}/show/{{prefix}}{{href}}">{{href}}</a></td>
                <td>{{newest-version}}</td>
                <td>{{last-version}}</td>
            </tr>
            {{/file}}
          </tbody>
        </table>
        {{/svn}}
      </div>

      <div class="app-oper">
        <a class="btn primary" href="/pub/pre">预发布</a>
        <a class="btn" href="/pub/beta">Beta发布</a>
        <a class="btn" href="/pub/formal">正式</a>
      </div>
