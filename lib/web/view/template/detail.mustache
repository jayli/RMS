      <div class="app-oper">
        <a class="btn primary" href="/{{apptype-href}}/{{app-name}}/publish/{{prefix}}?type=pre">预发布</a>
        <a class="btn" href="/{{apptype-href}}/{{app-name}}/publish/{{prefix}}?type=beta">Beta发布</a>
        <a class="btn" href="/{{apptype-href}}/{{app-name}}/publish/{{prefix}}?type=formal">正式</a>
      </div>
      <div class="app-list">
        <h3>
        <ul class="breadcrumb">
          <li><a href="/{{apptype-href}}/{{app-name}}/">{{app-name}}</a> <span class="divider">/</span>
          {{#pathes}}
          <li><a href="/{{apptype-href}}/{{app-name}}/show/{{href}}/">{{name}}</a> <span class="divider">/</span></li>
          {{/pathes}}
          <li class="active">{{app-filename}}</li>
        </ul>
        </h3>
        {{#svn}}
        {{#app-file}}
        {{> file}}
        {{/app-file}}
        {{^app-file}}
          <table class="zebra-striped">
            <thead>
              <tr>
                  <th class="choose">选中</th>
                  <th class="filename">文件名</th>
                  <th class="version">最新版本号</th>
                  <th class="version">发布版本号</th>
              </tr>
            </thead>
            <tbody>
              {{#dir}}
              <tr class="dir">
                  <td><span class="label success">F</span></td>
                  <td><a href="/{{apptype-href}}/{{app-name}}/show/{{prefix}}{{href}}">{{href}}</a></td>
                  <td>{{newest-version}}</td>
                  <td>{{last-version}}</td>
              </tr>
              {{/dir}}
              {{#file}}
              <tr>
                  <td><span class="label">F</span></td>
                  <td><a href="/{{apptype-href}}/{{app-name}}/show/{{prefix}}{{href}}">{{href}}</a></td>
                  <td>{{newest-version}}</td>
                  <td>{{last-version}}</td>
              </tr>
              {{/file}}
            </tbody>
          </table>
        {{/app-file}}
        {{/svn}}
      </div>
