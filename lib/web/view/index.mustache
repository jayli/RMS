<!doctype html>
<html>
  <head>
    <meta charset="utf8" />
    <title>RMS</title>
    <link href="/assets/rms.less" rel="stylesheet/less"/>
    <link rel="shortcut icon" href="http://a.tbcdn.cn/app/tms/favicon.ico" type="image/x-icon" />
  </head>
  <body>
      {{> topbar}}

    <div class="container">

      <div class="app-title page-header">
        <h2>Assets / detail</h2>
        <a href="#config" class="btn log">查看日志</a>
      </div>

      <div class="app-oper">
        <a class="btn primary" href="/pub/pre">预发布</a>
        <a class="btn" href="/pub/beta">Beta发布</a>
        <a class="btn" href="/pub/formal">正式</a>
      </div>

      <div class="app-list">
        <h3>
          <a href="#detail">detail</a> /
          <a href="#">lib</a> /
        </h3>
        <table class="zebra-striped">
          <thead>
            <tr>
                <th class="choose">选中</th>
                <th class="filename">文件名</th>
                <th class="version">最新版本号</th>
                <th class="version">发布版本号</th>
            </tr>
          </thead>
          <tbody>
            {{#list}}
            <tr>
                <td>#</td>
                <td><a href="base.js">base.js</a></td>
                <td>12345</td>
                <td>12344</td>
            </tr>
            {{/list}}
          </tbody>
        </table>
      </div>

      <div class="app-oper">
        <a class="btn primary" href="/pub/pre">预发布</a>
        <a class="btn" href="/pub/beta">Beta发布</a>
        <a class="btn" href="/pub/formal">正式</a>
      </div>

      <footer>
      RMS<sup>dev</sup> 2011-2012
      </footer>
    </div>
  <script src="http://a.tbcdn.cn/libs/seajs/1.0.2/sea.js" data-main="/assets/init"></script>
  </body>
</html>

